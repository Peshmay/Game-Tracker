{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/services/_tests_/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "src/utils/__mocks__/**"
  ]
}